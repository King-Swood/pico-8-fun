pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
actors={} -- all displayable items that may require collision
p1=nil -- should rarely be referenced
p2=nil -- should rarely be referenced

function _init()
	p1=player_new(0,0,120)
	p2=player_new(1,120,0)
end
	
function _update()
	for a in all(actors) do
		a.update(a)
	end
end
	
function _draw()
	cls()
	for a in all(actors) do
		a.draw(a)
	end
end

function actor_new(x,y)
	local a={}
	a.x=x
	a.y=y
	a.dx=0.0
	a.dy=0.0
	a.accel=0.8
	a.inertia=0.6
	a.spr=0
	a.frame=0
	a.frames=2
	a.framet=0
	a.frametmax=30
	a.flipx=false
	a.flipy=false
	a.w=1
	a.h=1
	a.pswap={}
 a.update=actor_update
 a.draw=actor_draw
 a.update_frame=actor_frame_speed
 add(actors,a)
	return a
end

function actor_draw(a)
	pal()	-- reset palette
	for orig,new in pairs(a.pswap) do
	 pal(orig,new)
	end
 spr(a.spr+a.frame,a.x,a.y,a.w,a.h,a.flipx)
end

function actor_update(a)
	actor_move(a)
	a.update_frame(a)
end

function actor_move(a)
	a.x+=a.dx
	a.y+=a.dy
 
 -- reduce speed each frame
 a.dx*=a.inertia
 a.dy*=a.inertia
	if (abs(a.dx)<0.2) a.dx=0
 if (abs(a.dy)<0.2) a.dy=0
end

function actor_frame_speed(a)
	a.framet+=1
 a.frame += abs(a.dx)
 a.frame += abs(a.dy)
 a.frame %= a.frames
	 
	-- moving
	if (abs(a.dx)>abs(a.dy))
	then
		-- moving left/right
	 a.flipx = (a.dx < 0)
	else
		-- moving towards/away from screen
	 a.flipy = (a.dy < 0)
	end
end


function actor_frame_time(a)
	a.framet+=1
	if (a.framet >= a.frametmax)
	then
		a.framet=0
	 a.frame+=1
	 a.frame%=a.frames
	end
	 
	-- moving
	if (abs(a.dx)>abs(a.dy))
	then
		-- moving left/right
	 a.flipx = (a.dx < 0)
	else
		-- moving towards/away from screen
	 a.flipy = (a.dy < 0)
	end
end

function player_new(no,x,y)
	p=actor_new(x,y)
	p.update=player_update
	p.update_frame=player_frame
	p.no=no
	p.shoottimer=0
	if (p.no==1)
	then
	 p.pswap[10]=14 -- palette swap for player 2
	end
	return p
end

function player_control(p)
 if (btn(0,p.no)) p.dx-=p.accel 
 if (btn(1,p.no)) p.dx+=p.accel 
 if (btn(2,p.no)) p.dy-=p.accel 
 if (btn(3,p.no)) p.dy+=p.accel 
 if (btnp(4,p.no))
 then
 	if (p.shoottimer==0)
 	then
	  mine_new(p.x,p.y)
	  p.shoottimer=30
	  end
 end
end

function player_update(p)
	player_control(p)
	actor_move(p)
	p.update_frame(p)
end

function player_frame(p)
	p.framet+=1
	if (p.dx==0 and p.dy==0)
	then
		-- standing still
		if (p.spr==0)
		then
			-- facing forward
			if (p.frame==0 and p.framet>90)
			then
				-- blink after timeout
			 p.frame=1
			 p.framet=0
			elseif (p.frame==1 and p.framet>5)
			then
				-- stop blinking after timeout
			 p.frame=0
			 p.framet=0
			end
		else
		-- facing away
		-- face forward after time period
			p.frame=0
			if (p.framet>15)
			then
			 p.spr=0
			 p.frame=0
				p.framet=0
			end
		end
	else
		-- moving
		if (abs(p.dx)>abs(p.dy))
		then
			-- moving left/right
		 p.spr=4
		 p.flipx = (p.dx < 0)
		elseif (p.dy > 0)
		then
			-- moving towards screen
		 p.spr=2
		else
			-- moving away from screen
			p.spr=6
		end
		p.frames=2
		p.framet=0
	 p.frame += abs(p.dx)
	 p.frame += abs(p.dy)
	 p.frame %= p.frames
	end
end

function mine_new(x,y,decoy)
	local m=actor_new(x,y)
	m.decoy=decoy
	m.spr=8
	m.frames=3
	m.frametmax=5
	m.update_frame=actor_frame_time
	return m
end

