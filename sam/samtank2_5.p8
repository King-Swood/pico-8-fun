pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
actors={}
sky=0
p1=0
p2=0

--added a direction to actor
--need to assign a direction to player based on button presses
--then when a rocket is fired we just pass the direction in to it
--the rocket should work out how to draw itself and which direction to fly in
--Add ability to remove an actor

function _init()
 p1=player_add(0,0,0)
 p2=player_add(50,50,1)
end

function _update()
 for a in all(actors) do
  a.update(a)
 end
end

-- we may not need 
function _draw()
 cls()
 --draw_sky()
 for a in all(actors) do
  if (a.draw==nil) then
   spr(a.spr+a.frame,a.x,a.y,1,1,a.flipx,a.flipy)
  else
   a.draw(a)
  end
 end
 print("p1 dir = "..p1.dir,64,120,7)
end

function draw_sky()
 map(0,0,sky,0,127,127)
 map(0,0,sky-256,0,127,127)
 sky+=1
 if(sky>=256)sky=0 
end

function actor_add(x,y)
 a={}
 a.x=x
 a.y=y
 a.dx=0
 a.dy=0
 a.spr=0
 a.frame=0
 a.frames=1
 a.framet=0
 a.frametmax=30
 a.flipx=false
 a.flipy=false
 a.remove=false
 a.dir=0 -- 0:n,1:e,2:s,3:w
 a.update=actor_update
 a.draw=nil
 add(actors,a)
 return a
end

function actor_update(a)
 a.x+=a.dx
 a.y+=a.dy
 a.framet+=1
 if(a.framet>=a.frametmax)
 then
  a.framet=0
  a.frame+=1
  if(a.frame>=a.frames)
  then
   a.frame=0
  end
 end
 a.flipy=(a.dir~=0)
 a.flipx=(a.dir~=1)
end

function actor_set_ds(a,dir,speed)
  a.dir=dir
 if (a.dir==0) then
   a.dy=(0-speed)
   a.dx=0
 elseif (a.dir==1) then
   a.dy=0
   a.dx=speed
 elseif (a.dir==2) then
   a.dy=speed
   a.dx=0
 else
   a.dy=0
   a.dx=(0-speed)
 end
end

function player_add(x,y,p_no)
 p=actor_add(x,y)
 p.spr=0
 p.frames=1
 p.frametmax=5
 p.update=player_update
 p.p_no=p_no
 return p
end

function player_control(p)
 p.dx=0
 p.dy=0
 if (btn(1,p.p_no)) then p.dx=1
 elseif (btn(0,p.p_no)) then p.dx=-1
 elseif (btn(3,p.p_no)) then p.dy=1
 elseif (btn(2,p.p_no)) then p.dy=-1
 end
 if (btnp(4,p.p_no))mine_add(p)
 --if (btnp(5,p.p_no))rocket_add(p)
 if (btnp(5,p.p_no))bullet_add(p)
end

function player_update(p)
 player_control(p)
 if (p.dx==0 and p.dy==0) then
  p.frame=0
  p.frames=1
  p.framet=0
 elseif (p.dy~=0) then
  p.spr=0
  p.frames=3
  if (p.dy > 0) then
   p.dir=2
  else
    p.dir=0
  end
 else
  p.spr=3
  p.frames=3
  if (p.dx < 0) then
   p.dir=3
  else
    p.dir=1
  end
 end
 actor_update(p)
end

function mine_add(p)
 a=actor_add(p.x,p.y)
 a.spr=16
 a.frames=2
 return a
end

function bullet_add(p)
 a=actor_add(p.x+3,p.y+3)
 actor_set_ds(a,p.dir,5)
 a.draw=bullet_draw
 return a
end

function bullet_draw(a)
 pset(a.x,a.y,9)
end

