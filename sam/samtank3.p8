pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
actors={}
sky=0
p1=0
p2=0
scr_actor=0

-- basic collision detection is working.
-- if players run into each other, both are deleted
-- if players touch an object, both player and object are deleted
-- however, whenever a bullet or mine is added, the player collides with it so they are both deleted.........

function _init()
 scr_actor=actor_create(0,0)
 scr_actor.hb.w=128
 scr_actor.hb.h=128
 p1=player_add(0,0,0)
 p2=player_add(50,50,1)
end

function _update()
 for a in all(actors) do
  if (a.remove) then del(actors,a)
  else a.update(a)
  end
 end
end

function _draw()
 cls()
 --draw_sky()
 for a in all(actors) do
  if (a.draw==nil) then
   spr(a.spr+a.frame,a.x,a.y,1,1,a.flipx,a.flipy)
  else
   a.draw(a)
  end
 end
--  print("p1 dir = "..p1.dir,64,120,7)
end

function draw_sky()
 map(0,0,sky,0,127,127)
 map(0,0,sky-256,0,127,127)
 sky+=1
 if(sky>=256)sky=0 
end

function actor_create(x,y)
 a={}
 a.x=x
 a.y=y
 a.dx=0
 a.dy=0
 a.hb={}
 a.hb.x=0
 a.hb.y=0
 a.hb.w=8
 a.hb.h=8
 a.spr=0
 a.frame=0
 a.frames=1
 a.framet=0
 a.frametmax=30
 a.flipx=false
 a.flipy=false
 a.remove=false
 a.dir=0 -- 0:none,1:n,2:e,3:s,4:w
 a.update=actor_update
 a.draw=nil
 return a
end

function actor_add(x,y)
 a=actor_create(x,y)
 add(actors,a)
 return a
end

function actor_update(a)
 a.x+=a.dx
 a.y+=a.dy
 a.framet+=1
 if(a.framet>=a.frametmax)
 then
  a.framet=0
  a.frame+=1
  if(a.frame>=a.frames)
  then
   a.frame=0
  end
 end
 if (a.dir~=0) then
  a.flipy=(a.dir~=1)
  a.flipx=(a.dir~=2)
 end
end

function actor_set_ds(a,dir,speed)
  a.dir=dir
 if (a.dir==1) then
   a.dy=(0-speed)
   a.dx=0
 elseif (a.dir==2) then
   a.dy=0
   a.dx=speed
 elseif (a.dir==3) then
   a.dy=speed
   a.dx=0
 elseif (a.dir==4) then
   a.dy=0
   a.dx=(0-speed)
 else
   a.dy=0
   a.dx=0
 end
end

function actor_hitbox_collision(a1,a2)
  if (a2.x+a2.hb.x+a2.hb.w > a1.x+a1.hb.x and 
    a2.y+a2.hb.y+a2.hb.h > a1.y+a1.hb.y and
    a2.x+a2.hb.x < a1.x+a1.hb.x+a1.hb.w and
    a2.y+a2.hb.y < a1.y+a1.hb.y+a1.hb.h)
  then
    return true
  end
end

function player_add(x,y,p_no)
 p=actor_add(x,y)
 p.spr=0
 p.frames=1
 p.frametmax=5
 p.update=player_update
 p.p_no=p_no
 return p
end

function player_control(p)
 if (btn(1,p.p_no)) then actor_set_ds(p,2,1)
 elseif (btn(0,p.p_no)) then actor_set_ds(p,4,1)
 elseif (btn(3,p.p_no)) then actor_set_ds(p,3,1)
 elseif (btn(2,p.p_no)) then actor_set_ds(p,1,1)
 else actor_set_ds(p,p.dir,0)
 end
 if (btnp(4,p.p_no))mine_add(p)
 if (btnp(5,p.p_no))bullet_add(p)
end

function player_update(p)
 player_control(p)
 if (p.dx==0 and p.dy==0) then
  p.frame=0
  p.frames=1
  p.framet=0
 elseif (p.dy~=0) then
  p.spr=0
  p.frames=3
 else
  p.spr=3
  p.frames=3
 end
 //player_collisions(p)
 actor_update(p)
end

function player_collisions(p)
 for a in all(actors) do
  if (a~=p) then
    if (actor_hitbox_collision(p,a)) then
      p.remove=true
      a.remove=true
    end
  end
 end
end

function mine_add(p)
 a=actor_add(p.x,p.y)
 a.spr=16
 a.frames=2
 return a
end

function bullet_add(p)
 a=actor_add(p.x+3,p.y+3)
 a.hb.w=1
 a.hb.h=1
 actor_set_ds(a,p.dir,5)
 a.draw=bullet_draw
 return a
end

function bullet_draw(a)
  if (actor_hitbox_collision(a,scr_actor)) then pset(a.x,a.y,9)
  else a.remove=true
  end
end

