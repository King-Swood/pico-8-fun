pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
p1=nil

function _init()
	p1=newplayer(10,10)
end
	
function _update()
	updateplayer(p1)
end
	
function _draw()
	cls()
	drawplayer(p1)
	
 print("x "..p1.x,0,120,7)
 print("y "..p1.y,64,120,7)
end

function newplayer(x,y)
	p={}
	p.x=0
	p.y=0
	p.dx=0.0
	p.dy=0.0
	p.accel=0.8
	p.inertia=0.6
	p.spr=0
	p.frame=0
	p.frames=2
	p.framet=0
	p.flipx=false
	p.w=1
	p.h=1
	return p
end

function drawplayer(p)
 spr(p.spr+p.frame,p.x,p.y,p.w,p.h,p.flipx)
end

function controlplayer(p)
 if (btn(0)) p.dx-=p.accel 
 if (btn(1)) p.dx+=p.accel 
 if (btn(2)) p.dy-=p.accel 
 if (btn(3)) p.dy+=p.accel 
end

function updateplayer(p)
	controlplayer(p1)
	moveplayer(p1)
	playerframe(p1)
end

function moveplayer(p)
	p.x+=p.dx
	p.y+=p.dy
 
 -- reduce speed each frame
 p.dx*=p.inertia
 p.dy*=p.inertia
	if (abs(p.dx)<0.2) p.dx=0
 if (abs(p.dy)<0.2) p.dy=0
end

function playerframe(p)
	p.framet+=1
	if (p.dx==0 and p.dy==0)
	then
		-- standing still
		if (p.spr==0)
		then
			-- facing forward
			if (p.frame==0 and p.framet>90)
			then
				-- blink after timeout
			 p.frame=1
			 p.framet=0
			elseif (p.frame==1 and p.framet>5)
			then
				-- stop blinking after timeout
			 p.frame=0
			 p.framet=0
			end
		else
		-- facing away
		-- face forward after time period
			p.frame=0
			if (p.framet>15)
			then
			 p.spr=0
			 p.frame=0
				p.framet=0
			end
		end
	else
		-- moving
		if (abs(p.dx)>abs(p.dy))
		then
			-- moving left/right
		 p.spr=4
		 p.flipx = (p.dx < 0)
		elseif (p.dy > 0)
		then
			-- moving towards screen
		 p.spr=2
		else
			-- moving away from screen
			p.spr=6
		end
		p.frames=2
		p.framet=0
	 p.frame += abs(p.dx)
	 p.frame += abs(p.dy)
	 p.frame %= p.frames
	end
end

